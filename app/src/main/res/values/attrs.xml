<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextProgressBar">
        <attr name="textColor" format="color"></attr>
        <attr name="textSize" format="dimension"></attr>
        <attr name="textFormat" format="string"></attr>
        <attr name="textBelow" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="Battery">
        <attr name="cap" format="integer"></attr>
        <attr name="charging" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="NumberOperator">
        <attr name="delta" format="integer"></attr>
        <attr name="value" format="integer"></attr>
    </declare-styleable>
</resources>
